{
  "service_configuration": {
    "name": "abu_dhabi_real_datasets",
    "title": "Abu Dhabi Real Infrastructure Datasets",
    "description": "Actual geographic datasets for Abu Dhabi city infrastructure and facilities",
    "service_type": "ClientSideFeatureCollection",
    "version": "1.0.0",
    "data_source": "OpenStreetMap / Ground Surveys"
