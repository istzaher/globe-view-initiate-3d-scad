      "id": "bus_stops_auh",
      "file": "knowledge_base/datasets/abu_dhabi/bus_stops_query.geojson",
      "title": "Abu Dhabi Bus Stops",
      "description": "Public transportation bus stops with ITC route information",
      "geometry_type": "Point",
      "category": "transportation",
      "icon": "bus",
      "color": "#2563eb",
      "fields": {
        "name_field": "name",
        "description_field": "name:en",
        "category_field": "highway",
        "id_field": "id"
      },
      "nlp_config": {
        "keywords": ["bus stop", "public transport", "transit", "ITC", "transportation"],
        "entities": {
          "TYPE": ["bus stop", "platform", "transit"],
          "OPERATOR": ["ITC", "Integrated Transport Centre"],
          "AMENITY": ["shelter", "bench", "lighting"]
        },
        "query_examples": [
          "Show all bus stops in Abu Dhabi",
          "Find bus stops with shelters",
          "List ITC bus stops",
          "Show bus stops near Emirates Palace"
        ]
      }
    },
